{
  "stryker-config": {
    "reporters": [
      "Html",
      "Progress",
      "ClearText"
    ],
    "log-level": "info",
    "timeout-ms": 10000,
    "log-file": true,
    "project-file": "TestApi.csproj",
    "max-concurrent-test-runners": 4,
    "threshold-high": 95,
    "threshold-low": 90,
    "threshold-break": 85,
    "files-to-exclude": [
    ],
    "excluded-mutations": [
    ],
    "ignore-methods": [
      "*LogDebug",
      "*LogTrace",
      "*LogError",
      "*LogInformation",
      "*LogWarning",
      "*TraceException"
    ],
    "mutate": [
      "!Startup.cs",
      "!Program.cs",
      "!ConfigureServicesExtensions.cs",
      "!Swagger/AuthResponsesOperationFilter.cs",
      "!ValidationMiddleware/RequestModelValidatorFilter.cs",
      "!ValidationMiddleware/RequestModelValidatorService.cs",
      "!Extensions/ApplicationLogger.cs",
      "!Extensions/EnumExtensions.cs",
      "!Telemetry/BadRequestTelemetry.cs",
      "!Telemetry/SuccessfulDependencyProcessor.cs",
      "!**/AssemblyInfo.cs"
    ]
  }
}
